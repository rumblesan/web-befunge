{
  "name": "web-befunge",
  "version": "0.1.0",
  "description": "A web based befunge interpreter",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/rumblesan/web-befunge.git"
  },
  "author": "Guy John",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "browserify . -o ./app.js",
    "watch": "watchify . -o ./app.js",
    "serve": "http-server ."
  },
  "browserify": {
    "transform": [
      ["babelify"]
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "http-server": "^0.8.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "underscore": "^1.8.3",
    "watchify": "^3.7.0"
  }
}
